<Window x:Class="bamsak.UploadResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="UploadResult" Height="300" Width="573">
    <Grid>
        <TextBox Height="35" HorizontalAlignment="Center" Margin="0,0,0,46" Name="linkTxt" VerticalAlignment="Bottom" Width="350" Background="#FFCEC9C9" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
        <Button Content="Null" Height="68" HorizontalAlignment="Left" Margin="112,43,0,0" Name="btn1" VerticalAlignment="Top" Width="153" Click="btn1_Click" />
        <Button Content="Null" Height="68" HorizontalAlignment="Left" Margin="298,43,0,0" Name="btn2" VerticalAlignment="Top" Width="153" Click="btn2_Click" />
        <ProgressBar Height="10" HorizontalAlignment="Center" Margin="0,143,0,0" Name="actionProgress" VerticalAlignment="Top" Width="200" IsIndeterminate="True" IsEnabled="True" />
        <Button Content="Copy to Clipboard" Height="23" HorizontalAlignment="Center" Margin="212,0,211,12" Name="btnClipboard" VerticalAlignment="Bottom" Width="128" Click="btnClipboard_Click" IsEnabled="False" />
        <StackPanel Height="100" HorizontalAlignment="Left" Margin="10,10,0,0" Name="stackPanel1" VerticalAlignment="Top" Width="200" />
    </Grid>
</Window>
